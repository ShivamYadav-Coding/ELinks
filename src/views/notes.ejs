     <div class="flex justify-center my-6"><i class="fas fa-user-friends cursor-pointer"></i></div>
     <div class="flex justify-center my-6"><i class="fas fa-user-shield cursor-pointer"></i></div>
  </div>
</section>

<div class= "p-4 overflow-y-auto flex flex-wrap rounded-xl w-full" style="min-height: 90vh; max-height: 90vh;">
  
  <div class=" h-48 w-40 m-4 rounded-lg hover:text-gray-400 text-8xl flex justify-center items-center text-gray-500 cursor-pointer bg-gray-700 ">
    <i class="fas fa-plus"></i>
  </div>

</div>

</div>


  <script src="/js/script.js"></script>
