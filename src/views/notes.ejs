<!doctype html class="m-0">
<html lang="en">
<head>
  <%- include('./partials/header.ejs') %>
<title>Home</title>
</head>
<body class="bg-gray-600 font-body box-border">

<!-- Model -->
<div class="fixed z-10 bg-gray-500 bg-opacity-50 flex justify-center items-center min-h-screen min-w-full inset-0 overflow-y-auto">

  <div class=" w-80 p-2 rounded-lg bg-gray-100 ">
    <div class="w-full p-4 flex items-center flex-col rounded-lg border-dashed border-2 border-gray-300"> 
          <i class="fas fa-truck-loading  text-5xl text-gray-500 transform translate-x-10"></i>
          <i class="fas fa-box-open text-7xl text-gray-500"></i>
          <div class="mt-4">Drop your files here or, <span class="text-indigo-500">browse</span></div>
    </div>
  </div>

</div>

   <!-- Navbar -->
   <%- include('./partials/navbar.ejs') %>
  
<div class="flex" id="mainNotesContent">
