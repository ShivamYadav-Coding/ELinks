  <section id="sidenav" class="flex min-h-full">
  
  <div class="max-h-full min-h-full w-48 flex flex-col bg-gray-700 overflow-x-auto overflow-y-auto" id="tags">
       

  </div>

  <div class="max-h-full min-h-full w-16 bg-gray-800 text-gray-100 text-4xl justify-center overflow-x-auto overflow-y-auto">
